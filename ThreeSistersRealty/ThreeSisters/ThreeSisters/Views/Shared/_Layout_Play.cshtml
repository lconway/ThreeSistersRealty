<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/modernizr")

    <!-- DataTables CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/themes/base/jquery.dataTables.css">

    <!-- jQuery -->
    <script type="text/javascript" charset="utf8" src="~/Scripts/jquery.js"></script>
  
    <!-- DataTables -->
    <script type="text/javascript" charset="utf8" src="~/Scripts/jquery.dataTables.js"></script>
</head>
<body>
    <div id="div1">
        <h1 id="Title">Three Sisters Realty</h1>
        <img id="TopImage" width="610" height="150" src="~/Images/3Sisters pan 163KB.jpg" />
    </div>
    <div class="clear" />
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
